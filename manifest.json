{
  "manifest_version": 2,
  "name": "FlowWatch",
  "version": "1.0",
  "description": "A stopwatch Chrome extension with Google Calendar integration.",
  "key": "",
  "browser_action": {
    "default_icon": {
      "48": "images/icon48.png",
      "96": "images/icon96.png"
    }
  },
  "permissions": [
    "alarms",
    "storage",
    "activeTab",
    "identity",
     "https://www.googleapis.com/"
  ],
  "oauth2": {
    "client_id": "831012186265-2nql9kresh3o4nkf4udrjprnqns0rpec.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar"
    ]
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "48": "images/icon48.png",
    "96": "images/icon96.png"
  },
  "web_accessible_resources": [
    "audio/focus_compressed.opus"
  ],
  "options_page": "record.html",
  "content_security_policy": "script-src 'self' https://d3js.org; object-src 'self'",
  "content_security_policy": "script-src 'self' https://d3js.org https://unpkg.com; object-src 'self'",
  "web_accessible_resources": [
    "audio/focus_compressed.opus"
  ]
}
